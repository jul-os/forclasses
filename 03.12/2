class Animal():
    def __init__(self, nme):
        self.othername=nme
        
class WildAnimal(Animal):
    def __init__(self,env):
        self.evironment=env
class WaterAnimal(WildAnimal):
    def __init__(self, salt):
        self.watersalinity=salt
class ForestAnimal(WildAnimal):
    def __init__(self,frst):
        self.foresttype=frst
class HomeAnimal(Animal):
    def __init__(self, tob):
        self.whattoby=tob   
class Pets(HomeAnimal):
    def __init__(self,play):
        self.toy=play
class Livestock(HomeAnimal):
    def __init__(self, need):
        self.needs=need
class Cow(Livestock):
    nme='Bos Taurus'
    tob='food'
    need='cowshed'
class Wolf(ForestAnimal):
    nme='Canis Lupus'
    env='eurasia and north america'
    frst='doesnt like it actually'
wolf=Wolf()
print(wolf.foresttype)
