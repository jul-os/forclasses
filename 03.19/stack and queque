class Stack():
    def __init__(self, array):
        if len(array)!=0:
            self.array=array
        else:
            self.array=[]
    def append(self, app):
        self.array.insert(0,app)
        return self.array
    def pop(self):
        del self.array[0]
        return self.array
    def peek(self):
        return self.array[0]
    
class Queque():
    def __init__(self, array):
        if len(array)!=0:
            self.array=array
        else:
            self.array=[]
    def append(self, app):
        self.array.insert(0,app)
        return self.array
    def pop(self):
        del self.array[-1]
        return self.array
    def peek(self):
        return self.array[-1]
    
a=[3,2,1]
b=[1,2,3]
a=Stack(a)
b=Queque(b)
print(a.append(4))
print(a.peek())
print(a.pop())
print('\n')
print(b.append(4))
print(b.pop())
print(b.peek())
